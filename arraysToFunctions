public class ArraysToFunctions {
    static int getArraySum(int[] A)
    {
        int sum=0;
        for(int i=0;i<A.length;i++)
        {
            sum+=A[i];
        }
        return sum;
    }
    public static void main(String[] args)
    {
        int[] A={10,20,30};
        int[] B={1,2,3,4,5,6};
        int[] C={-1,-14,-19,12};
        System.out.println(getArraySum(A));
        System.out.println(getArraySum(B));
        System.out.println(getArraySum(C));
    }
}
